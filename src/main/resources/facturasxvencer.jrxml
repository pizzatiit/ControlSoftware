	<queryString>
		<![CDATA[select f.idfactura as id, f.numerofactura as nfactura,f.fechafactura as fecha_factura,f.total as total, f.saldo as saldo, c.nombre as cliente, c.diaslimite as diascredito, (c.diaslimite - DATEDIFF(NOW(),f.fechafactura)) as dias_restantes,v.vendedor as vendedor, DATE_ADD(f.fechafactura, INTERVAL c.diaslimite DAY) as fecha_vencimiento from factura f inner join clientes c on c.idcliente=f.idcliente inner join company co on co.id_company=f.cia inner join vendedor v on v.id_vendedor=f.idvendedor where (c.diaslimite - DATEDIFF(NOW(),f.fechafactura))<=co.alertavencimiento and f.idvendedor= 11 and f.cia= 1;]]>
