	<queryString>
		<![CDATA[SELECT O.IDORDEN AS ID,V.VENDEDOR, O.NUMERO_ORDEN AS NORDEN, CASE O.TIPO WHEN '0' THEN 'PROFORMA' WHEN '1' THEN 'ORIGINAL' END AS TIPO, O.FECHAORDEN AS FECHA_PEDIDO, CASE O.CONDICIONPAGO WHEN '0' THEN 'CRÉDITO' WHEN '1' THEN 'CONTADO' END AS CONDICION_PAGO, C. NOMBRE AS CLIENTE,C.RTN AS RTN, CI.CIUDAD AS CIUDAD, D.DEPARTAMENTO AS DEPTO, P.CODIGO,P.DESCRIPCION,DO.IDPRODUCTO AS IDPROD, DO.CANTIDAD,DO.PRECIOVENDIDO, COSTO_EN_LETRAS(DO.COSTO) AS COSTO, o.total as TOTAL,do.cantidad*do.preciovendido as totalparcial FROM ORDEN_DE_PEDIDO O INNER JOIN DETALLE_ORDEN_DE_PEDIDO DO ON DO.ID_ORDEN_DE_PEDIDO=O.IDORDEN INNER JOIN CLIENTES C ON C.IDCLIENTE=O.IDCLIENTE LEFT JOIN CIUDAD CI ON CI.ID_CIUDAD=C.CIUDAD LEFT JOIN DEPARTAMENTO D ON D.ID_DEPARTAMENTO=C.DEPARTAMENTO INNER JOIN PRODUCTO P ON P.IDPRODUCTO=DO.IDPRODUCTO INNER JOIN VENDEDOR V ON V.ID_VENDEDOR=O.IDVENDEDOR WHERE O.IDORDEN=$P{pedido} AND O.CIA=$P{cia}]]>
	</queryString>
