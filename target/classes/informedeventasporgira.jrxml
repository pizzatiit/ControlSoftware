	<queryString>
		<![CDATA[select f.numerofactura, sum(d.costo*d.cantidad) as costo, if(f.tipo=1,  f.subtotal-sum(d.costo*d.cantidad),f.total-sum(d.costo*d.cantidad)) as utilidad,(if(f.tipo=1, f.subtotal-sum(d.costo*d.cantidad),f.total-sum(d.costo*d.cantidad))/f.total)*100 as porcentaje, case f.condicionpago when '0' then 'CRE' WHEN '1' THEN 'CTD' END AS condicionpago, f.fechafactura, co.img, co.company,v.vendedor, c.nombre, f.subtotal, f. isv, f.total, g.gira, g.id_gira from factura f inner join detalle_factura d on d.idfactura=f.idfactura inner join company co on co.id_company=f.cia inner join clientes c on c.idcliente=f.idcliente inner join vendedor v on v.id_vendedor=f.idvendedor inner join gira g on g.id_gira=c.gira where g.cia=$P!{cia} and f.estado!=0 and f.fechafactura between $P{dfecha} and $P{hfecha} and (g.id_gira between $P{idvendedordesde} and $P{idvendedorhasta}) and f.tipo between $P!{tipof} and $P!{tipof2} and f.condicionpago between $P!{tipop} and $P{tipop2} group by g.id_gira,f.idfactura order by g.id_gira, cast(f.numerofactura as unsigned)]]>
