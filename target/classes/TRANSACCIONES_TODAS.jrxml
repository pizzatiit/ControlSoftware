    COALESCE((SELECT costo from detalle_factura where detalle_factura.idfactura=kardex.id_documento and detalle_factura.idproducto=kardex.id_producto), 'DOC. ELIMINADO') as costo,
    COALESCE((SELECT preciovendido from detalle_factura where detalle_factura.idfactura=kardex.id_documento and detalle_factura.idproducto=kardex.id_producto), 'DOC. ELIMINADO') as precio,
    saldo_actual,
