    COALESCE((SELECT observacion FROM factura WHERE factura.idfactura = kardex.id_documento), 'DOC. ELIMINADO') AS observacion
FROM
    kardex
WHERE
    id_producto = $P!{producto} and tipo_documento=2 and cia=$P!{cia}  

UNION

