	<queryString>
		<![CDATA[select op.numero_orden, co.company,co.direccion as direccioncia, co.rtn as rtncia, co.img, v.vendedor, c.nombre,c.rtn, c.numerotelefono, c.direccion as dir, ci.ciudad, de.departamento, op.subtotal, op.isv, op.total, op.fechaorden, op.fechaingreso,op.observacion, u.user, CASE op.condicionpago WHEN '1' THEN 'CONTADO' WHEN '0' THEN 'CREDITO' END as condicionpago, d.idproducto, p.codigo, p.descripcion, d.cantidad, d.preciovendido as preciovendido, d.total as totalp from orden_de_pedido op inner join company co on op.cia=co.id_company inner join vendedor v on op.idvendedor=v.id_vendedor inner join clientes c on c.idcliente=op.idcliente inner join usuario u on u.cod_user=op.idusuario inner join detalle_orden_de_pedido d on d.id_orden_de_pedido=op.idorden inner join producto p on p.idproducto=d.idproducto inner join ciudad ci on ci.id_ciudad=c.ciudad inner join departamento de on de.id_departamento=c.departamento where op.idorden=$P!{factura} and op.cia=$P!{cia}]]>
